  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ITI_DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SID">
            <Value>=Parameters!SID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select S.St_Fname,S.St_Lname , SC.Grade ,C.Crs_Name 
from Student S inner join Stud_Course SC
on S.St_Id = SC.St_Id 
inner join Course C 
on SC.Crs_Id = C.Crs_Id
where  S.St_Id = @SID
order by SC.Grade</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
